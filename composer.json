{
  "name": "emiliopedrollo/laravel-secure-passwords",
  "description": "Provide useful ways to ensure strong passwords via validation",
  "keywords": [
    "laravel", "password", "validation", "security"
  ],
  "homepage": "https://github.com/emiliopedrollo/secure-passwords",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Em√≠lio B. Pedrollo",
      "email": "emiliopedrollo@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/emiliopedrollo/secure-passwords/issues"
  },
  "require": {
    "php": "^7.2|^8.0",
    "laravel/framework": "^6.20.14|^7.30.4|^8.24.0"
  },
  "require-dev":{
    "laravel/laravel": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pedrollo\\SecurePasswords\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pedrollo\\SecurePasswords\\Tests\\": "tests/"
    }
  },
  "extra":{
    "laravel": {
      "providers": [
        "Pedrollo\\SecurePasswords\\SecurePasswordsServiceProvider"
      ]
    }
  }
}
